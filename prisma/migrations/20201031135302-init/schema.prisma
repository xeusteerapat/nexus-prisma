// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          String @id @default(uuid())
  name        String
  description String
  price       Int
  sku         String @unique
}